

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Listagem de Tipos de Consultas</title>
  <link rel="stylesheet" href="/styles/style.css">
</head>
<body>
  <h1>Listagem de Tipos de Consultas</h1>

  <ul>
    <% tiposConsultas.forEach(tipoConsulta => { %>
      <li>
        <strong>ID:</strong> <%= tipoConsulta.id %>,
        <strong>Nome:</strong> <%= tipoConsulta.nome %>
      </li>
    <% }); %>
  </ul>
</body>
</html>
